{
  "version": 2,
  "builds": [
    {
      "src": "backoffice/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontoffice/film-project/dist/**/*", 
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backoffice/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontoffice/film-project/dist/index.html" 
    }
  ]
}
